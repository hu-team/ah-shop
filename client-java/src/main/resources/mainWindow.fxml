<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<AnchorPane minHeight="350.0" minWidth="600.0" prefHeight="350.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.arvici.client.controller.MainWindow">
   <children>
      <TabPane prefHeight="350.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Login">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                         <Label layoutX="14.0" layoutY="18.0" text="Username" />
                         <TextField fx:id="loginUsername" layoutX="151.0" layoutY="14.0" />
                         <Label layoutX="14.0" layoutY="60.0" text="Password" />
                         <PasswordField fx:id="loginPassword" layoutX="151.0" layoutY="56.0" />
                         <Button fx:id="loginButton" layoutX="151.0" layoutY="96.0" mnemonicParsing="false" onAction="#onLogin" text="Login" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Registrer">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                         <Button fx:id="registerButton" layoutX="151.0" layoutY="136.0" mnemonicParsing="false" onAction="#onRegister" text="Register" />
                         <TextField fx:id="registerUsername" layoutX="151.0" layoutY="10.0" />
                         <Label layoutX="15.0" layoutY="50.0" text="Password" />
                         <Label layoutX="15.0" layoutY="14.0" text="Username" />
                         <TextField fx:id="registerPassword" disable="true" editable="false" layoutX="151.0" layoutY="46.0" text="Generated password" />
                         <Label layoutX="15.0" layoutY="98.0" text="Name" />
                         <TextField fx:id="registerName" layoutX="151.0" layoutY="94.0" />
                         <TextArea fx:id="registerOutput" editable="false" layoutX="315.0" layoutY="10.0" prefHeight="301.0" prefWidth="276.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
